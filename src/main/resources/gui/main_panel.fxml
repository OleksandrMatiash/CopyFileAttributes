<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="450.0" prefWidth="1200.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vortex.fileattributes.Controller">
    <children>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="10.0">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#upButtonClicked" prefWidth="110.0" text="UP">
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints />
            <RowConstraints />
        </rowConstraints>
         <children>
            <ChoiceBox fx:id="dstCB1" prefHeight="25.0" prefWidth="1000.0" GridPane.columnIndex="1" />
            <TextField fx:id="srcTF1" editable="false" />
            <ChoiceBox fx:id="dstCB2" layoutX="350.0" layoutY="13.0" prefHeight="25.0" prefWidth="1000.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="srcTF2" editable="false" layoutX="10.0" layoutY="13.0" GridPane.rowIndex="1" />
            <ChoiceBox fx:id="dstCB3" layoutX="350.0" layoutY="13.0" prefWidth="1000.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="srcTF3" editable="false" layoutX="10.0" layoutY="13.0" GridPane.rowIndex="2" />
            <ChoiceBox fx:id="dstCB4" layoutX="350.0" layoutY="13.0" prefWidth="1000.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="srcTF4" editable="false" layoutX="10.0" layoutY="13.0" GridPane.rowIndex="3" />
            <ChoiceBox fx:id="dstCB5" layoutX="350.0" layoutY="13.0" prefWidth="1000.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="srcTF5" editable="false" layoutX="10.0" layoutY="13.0" GridPane.rowIndex="4" />
            <ChoiceBox fx:id="dstCB6" layoutX="350.0" layoutY="13.0" prefWidth="1000.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="srcTF6" editable="false" layoutX="10.0" layoutY="13.0" GridPane.rowIndex="5" />
            <ChoiceBox fx:id="dstCB7" layoutX="350.0" layoutY="13.0" prefWidth="1000.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextField fx:id="srcTF7" editable="false" layoutX="10.0" layoutY="13.0" GridPane.rowIndex="6" />
         </children>
      </GridPane>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="10.0">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#downButtonClicked" prefWidth="110.0" text="DOWN">
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </Button>
            <Button layoutX="20.0" layoutY="20.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#copyButtonClicked" prefWidth="150.0" text="COPY ATTRIBUTES">
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <TextArea editable="false" prefHeight="100.0" prefWidth="200.0" text="1. Drag and drop SRC files (attributes will be copied FROM these files) to the left column. &#10;2. Drag and drop DSD files (attributes will be copied from SRC files) to the right column. &#10;3. Match files. &#10;4. Copy attributes. Matched files will be removed from both lists after this procedure." />
    </children>
    <padding>
        <Insets bottom="30.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
